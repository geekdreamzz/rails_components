<div class="divide-y divide-gray-200 px-4 sm:px-6">
	<div class="space-y-6">

		<div>
<!--			<h3 class="text-sm font-medium leading-6 text-gray-900">Blank Chat Test</h3>-->
			<div class="mt-3">
				<% if local_assigns[:thumb] %>
					<div class="flex space-x-2">
						<a href="#" class="rounded-full hover:opacity-75">
							<img class="inline-block h-8 w-8 rounded-full" src="<%= thumb %>">
						</a>
					</div>
				<% end %>

				<div class="mt-1 ml-1">
					<% if local_assigns[:msg] %>
						<%= msg %>
					<% end %>
				</div>

			</div>
		</div>

	</div>
</div>
